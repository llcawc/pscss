#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/www/blueprints/pscss/tests/node_modules/.pnpm/@pasmurno+serve@0.0.4_@types+node@24.9.1_terser@5.44.0/node_modules/@pasmurno/serve/bin/node_modules:/proc/cygdrive/c/www/blueprints/pscss/tests/node_modules/.pnpm/@pasmurno+serve@0.0.4_@types+node@24.9.1_terser@5.44.0/node_modules/@pasmurno/serve/node_modules:/proc/cygdrive/c/www/blueprints/pscss/tests/node_modules/.pnpm/@pasmurno+serve@0.0.4_@types+node@24.9.1_terser@5.44.0/node_modules/@pasmurno/node_modules:/proc/cygdrive/c/www/blueprints/pscss/tests/node_modules/.pnpm/@pasmurno+serve@0.0.4_@types+node@24.9.1_terser@5.44.0/node_modules:/proc/cygdrive/c/www/blueprints/pscss/tests/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/www/blueprints/pscss/tests/node_modules/.pnpm/@pasmurno+serve@0.0.4_@types+node@24.9.1_terser@5.44.0/node_modules/@pasmurno/serve/bin/node_modules:/proc/cygdrive/c/www/blueprints/pscss/tests/node_modules/.pnpm/@pasmurno+serve@0.0.4_@types+node@24.9.1_terser@5.44.0/node_modules/@pasmurno/serve/node_modules:/proc/cygdrive/c/www/blueprints/pscss/tests/node_modules/.pnpm/@pasmurno+serve@0.0.4_@types+node@24.9.1_terser@5.44.0/node_modules/@pasmurno/node_modules:/proc/cygdrive/c/www/blueprints/pscss/tests/node_modules/.pnpm/@pasmurno+serve@0.0.4_@types+node@24.9.1_terser@5.44.0/node_modules:/proc/cygdrive/c/www/blueprints/pscss/tests/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/serve.js" "$@"
else
  exec node  "$basedir/../../bin/serve.js" "$@"
fi
